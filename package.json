    "ts-node": "10.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@google/genai": "^1.12.0",
    "@mozilla/readability": "^0.6.0",
    "@types/jsdom": "^21.1.7",
